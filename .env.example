# QuantConnect API Configuration
# Copy this file to .env and fill in your actual values

# QuantConnect API credentials
QC_USER_ID=your_user_id_here
QC_API_TOKEN=your_api_token_here

# Optional: For local LEAN CLI
QC_DATA_FOLDER=/path/to/data
QC_RESULTS_FOLDER=/path/to/results

# Optional: Brokerage API keys (for live trading)
# ALPACA_API_KEY=your_alpaca_key
# ALPACA_SECRET_KEY=your_alpaca_secret
# INTERACTIVE_BROKERS_ACCOUNT=your_ib_account
# INTERACTIVE_BROKERS_USER=your_ib_user
# INTERACTIVE_BROKERS_PASSWORD=your_ib_password

# Development settings
DEBUG=True
LOG_LEVEL=INFO

# =====================================
# UPGRADE-015: Observability & Agent Ops
# =====================================

# AgentOps API key for AI agent monitoring
# Get your key at: https://app.agentops.ai/
AGENTOPS_API_KEY=your_agentops_api_key_here

# Optional: OpenTelemetry configuration
# OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
# OTEL_SERVICE_NAME=quantconnect-trading-bot
